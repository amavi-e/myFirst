<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myfirst.LoginController">
   <children>
      <Label layoutX="116.0" layoutY="140.0" text="Username">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="117.0" layoutY="201.0" text="Password">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordText" layoutX="203.0" layoutY="197.0" prefHeight="26.0" prefWidth="233.0">
         <cursor>
            <Cursor fx:constant="NONE" />
         </cursor>
      </PasswordField>
      <TextField fx:id="usernameText" layoutX="203.0" layoutY="136.0" prefHeight="26.0" prefWidth="233.0" />
      <Button layoutX="252.0" layoutY="257.0" mnemonicParsing="false" onAction="#onLoginButtonClick" text="Login" />
      <Label fx:id="myspacelabel" layoutX="215.0" layoutY="80.0" text="My Space">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <MenuBar layoutX="385.0" layoutY="14.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="closeMenuButton" mnemonicParsing="false" onAction="#onHelloButtonClick" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
