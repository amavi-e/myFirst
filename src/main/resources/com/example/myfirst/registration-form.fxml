<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myfirst.RegistrationController">
    <children>
        <Label layoutX="91.0" layoutY="133.0" text="Full Name :">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="91.0" layoutY="192.0" text="   Email ID :">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="91.0" layoutY="258.0" text="Password :">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextField fx:id="fullNameText" layoutX="178.0" layoutY="130.0" prefHeight="26.0" prefWidth="365.0">
            <cursor>
                <Cursor fx:constant="DEFAULT" />
            </cursor></TextField>
        <TextField fx:id="EmailIdText" layoutX="178.0" layoutY="189.0" prefHeight="26.0" prefWidth="365.0">
            <cursor>
                <Cursor fx:constant="NONE" />
            </cursor></TextField>
        <PasswordField fx:id="passwordText" layoutX="178.0" layoutY="255.0" prefHeight="26.0" prefWidth="365.0">
            <cursor>
                <Cursor fx:constant="NONE" />
            </cursor></PasswordField>
        <Label layoutX="200.0" layoutY="64.0" text="Registration Form" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="27.0" />
            </font>
        </Label>
        <Button fx:id="submitButton" layoutX="248.0" layoutY="306.0" mnemonicParsing="false" onAction="#onSubmitButtonClick" prefHeight="41.0" prefWidth="103.0" text="Submit" />
    </children>
</AnchorPane>
